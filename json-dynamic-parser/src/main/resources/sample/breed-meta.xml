<?xml version='1.0' encoding='utf-8'?>
<configuration  xmlns="http://json.config.ns">
    <data type="json">
        <file-path resources-path="true">/sample/data.json</file-path>
        <entries>
            <entry>
                <label>names</label>
                <domain-class>com.lapots.breed.platform.json.sample.NameDomain</domain-class>
                <repository-class>com.lapots.breed.platform.json.sample.NameRepository</repository-class>
            </entry>
        </entries>
    </data>
    <converters>
        <converter class="com.lapots.breed.platform.json.sample.converter.StringToNameDomainSingleConverter">
            <from-class>java.lang.String</from-class>
            <to-class>com.lapots.breed.platform.json.sample.NameDomain</to-class>
        </converter>
    </converters>
</configuration>
